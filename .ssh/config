Host *
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519

Host github.com
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519_github

Host 10.0.0.4
  HostName 10.0.0.4
  User root
  
Host 10.1.*
  User admin
  UseKeychain yes
  AddKeysToAgent yes
  StrictHostKeyChecking no
  IdentityFile ~/.ssh/id_ed25519_aws
